{
  "name": "imbricate",
  "displayName": "imbricate",
  "description": "Code extension for Imbricate",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "license": "MIT",
  "categories": [
    "Notebooks"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "imbricate",
          "title": "Imbricate",
          "icon": "$(primitive-square)"
        }
      ]
    },
    "views": {
      "imbricate": [
        {
          "id": "imbricate-editings",
          "name": "Imbricate Editings",
          "icon": "$(settings-edit)",
          "contextualTitle": "Imbricate Editings"
        },
        {
          "id": "imbricate-pages",
          "name": "Imbricate Pages",
          "icon": "$(remote-explorer-documentation)",
          "contextualTitle": "Imbricate Pages"
        },
        {
          "id": "imbricate-scripts",
          "name": "Imbricate Scripts",
          "icon": "$(terminal-view-icon)",
          "contextualTitle": "Imbricate Scripts"
        }
      ]
    },
    "commands": [
      {
        "command": "imbricate.editings.refresh",
        "title": "Imbricate Editings Refresh",
        "icon": "$(timeline-refresh)"
      },
      {
        "command": "imbricate.editing.resume",
        "title": "Imbricate Editing Resume",
        "icon": "$(settings-edit)"
      },
      {
        "command": "imbricate.page.preview",
        "title": "Imbricate Page Preview",
        "icon": "$(notebook-render-output)"
      },
      {
        "command": "imbricate.pages.refresh",
        "title": "Imbricate Pages Refresh",
        "icon": "$(timeline-refresh)"
      },
      {
        "command": "imbricate.page.edit",
        "title": "Imbricate Page Edit",
        "icon": "$(settings-edit)"
      },
      {
        "command": "imbricate.script.edit",
        "title": "Imbricate Script Edit",
        "icon": "$(settings-edit)"
      },
      {
        "command": "imbricate.scripts.refresh",
        "title": "Imbricate Scripts Refresh",
        "icon": "$(timeline-refresh)"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "imbricate.page.edit",
          "when": "false"
        }
      ],
      "view/title": [
        {
          "command": "imbricate.editings.refresh",
          "when": "view == imbricate-editings",
          "group": "navigation"
        },
        {
          "command": "imbricate.pages.refresh",
          "when": "view == imbricate-pages",
          "group": "navigation"
        },
        {
          "command": "imbricate.scripts.refresh",
          "when": "view == imbricate-scripts",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "imbricate.page.preview",
          "when": "view == imbricate-pages && viewItem == page-item",
          "group": "inline"
        },
        {
          "command": "imbricate.page.edit",
          "when": "view == imbricate-pages && viewItem == page-item",
          "group": "inline"
        },
        {
          "command": "imbricate.script.edit",
          "when": "view == imbricate-scripts && viewItem == script-item",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "dependencies": {
    "@imbricate/core": "^1.10.0",
    "@imbricate/local-fundamental": "^1.1.0",
    "@imbricate/markdown-render": "^1.1.2",
    "@imbricate/origin-file-system": "^1.8.0",
    "@sudoo/io": "^1.8.0"
  },
  "devDependencies": {
    "@types/mocha": "^10",
    "@types/node": "20",
    "@types/vscode": "^1",
    "@typescript-eslint/eslint-plugin": "^7",
    "@typescript-eslint/parser": "^7",
    "@vscode/test-cli": "^0",
    "@vscode/test-electron": "^2",
    "eslint": "^8",
    "typescript": "^5"
  }
}