{
  "name": "imbricate",
  "packageManager": "yarn@4.1.1",
  "displayName": "imbricate",
  "description": "Code extension for Imbricate",
  "version": "0.0.1",
  "type": "commonjs",
  "engines": {
    "vscode": "^1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "imbricate.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10",
    "@types/node": "18",
    "@types/vscode": "^1",
    "@typescript-eslint/eslint-plugin": "^7",
    "@typescript-eslint/parser": "^7",
    "@vscode/test-cli": "^0",
    "@vscode/test-electron": "^2",
    "eslint": "^8",
    "typescript": "^5"
  }
}
