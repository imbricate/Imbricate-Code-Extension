{
  "name": "imbricate",
  "version": "1.2.0",
  "displayName": "imbricate",
  "icon": "images/imbricate.png",
  "author": "WMXPY",
  "license": "MIT",
  "private": true,
  "publisher": "imbricate",
  "description": "Yes to accountable notes, no to external hosting. Free, for everyone.",
  "engines": {
    "vscode": "^1.76.0"
  },
  "bugs": {
    "url": "https://github.com/Imbricate/Imbricate/issues"
  },
  "homepage": "https://imbricate.io",
  "categories": [
    "Notebooks"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Imbricate/Imbricate.git"
  },
  "keywords": [
    "imbricate",
    "note"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "imbricate",
          "title": "Imbricate",
          "icon": "$(primitive-square)"
        }
      ]
    },
    "views": {
      "imbricate": [
        {
          "id": "imbricate-editings",
          "name": "Imbricate Editings",
          "icon": "$(settings-edit)",
          "contextualTitle": "Imbricate Editings"
        },
        {
          "id": "imbricate-pages",
          "name": "Imbricate Pages",
          "icon": "$(remote-explorer-documentation)",
          "contextualTitle": "Imbricate Pages"
        },
        {
          "id": "imbricate-scripts",
          "name": "Imbricate Scripts",
          "icon": "$(terminal-view-icon)",
          "contextualTitle": "Imbricate Scripts"
        }
      ]
    },
    "commands": [
      {
        "command": "imbricate.editings.perform-all",
        "title": "Perform All Imbricate Editings",
        "category": "Imbricate",
        "icon": "$(save-all)"
      },
      {
        "command": "imbricate.editing.perform",
        "title": "Perform Editing",
        "category": "Imbricate",
        "icon": "$(save)"
      },
      {
        "command": "imbricate.editings.refresh",
        "title": "Editings Refresh",
        "category": "Imbricate",
        "icon": "$(timeline-refresh)"
      },
      {
        "command": "imbricate.editing.resume",
        "title": "Editing Resume",
        "category": "Imbricate",
        "icon": "$(settings-edit)"
      },
      {
        "command": "imbricate.page.create",
        "title": "Create Imbricate Page",
        "category": "Imbricate",
        "icon": "$(add)"
      },
      {
        "command": "imbricate.page.preview",
        "title": "Page Preview",
        "category": "Imbricate",
        "icon": "$(notebook-render-output)"
      },
      {
        "command": "imbricate.pages.refresh",
        "title": "Pages Refresh",
        "category": "Imbricate",
        "icon": "$(timeline-refresh)"
      },
      {
        "command": "imbricate.page.edit",
        "title": "Page Edit",
        "category": "Imbricate",
        "icon": "$(settings-edit)"
      },
      {
        "command": "imbricate.script.create",
        "title": "Create Imbricate Script",
        "category": "Imbricate",
        "icon": "$(add)"
      },
      {
        "command": "imbricate.script.edit",
        "title": "Script Edit",
        "category": "Imbricate",
        "icon": "$(settings-edit)"
      },
      {
        "command": "imbricate.script.preview",
        "title": "Script Preview",
        "category": "Imbricate",
        "icon": "$(notebook-render-output)"
      },
      {
        "command": "imbricate.scripts.refresh",
        "title": "Scripts Refresh",
        "category": "Imbricate",
        "icon": "$(timeline-refresh)"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "imbricate.editings.refresh",
          "when": "false"
        },
        {
          "command": "imbricate.editing.resume",
          "when": "false"
        },
        {
          "command": "imbricate.editing.perform",
          "when": "false"
        },
        {
          "command": "imbricate.page.preview",
          "when": "false"
        },
        {
          "command": "imbricate.page.edit",
          "when": "false"
        },
        {
          "command": "imbricate.pages.refresh",
          "when": "false"
        },
        {
          "command": "imbricate.script.edit",
          "when": "false"
        },
        {
          "command": "imbricate.script.preview",
          "when": "false"
        },
        {
          "command": "imbricate.scripts.refresh",
          "when": "false"
        }
      ],
      "view/title": [
        {
          "command": "imbricate.editings.perform-all",
          "when": "view == imbricate-editings",
          "group": "navigation"
        },
        {
          "command": "imbricate.editings.refresh",
          "when": "view == imbricate-editings",
          "group": "navigation"
        },
        {
          "command": "imbricate.pages.refresh",
          "when": "view == imbricate-pages",
          "group": "navigation"
        },
        {
          "command": "imbricate.scripts.refresh",
          "when": "view == imbricate-scripts",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "imbricate.editing.perform",
          "when": "view == imbricate-editings && viewItem == editing-item",
          "group": "inline"
        },
        {
          "command": "imbricate.page.create",
          "when": "view == imbricate-pages && viewItem == page-collection-item",
          "group": "inline"
        },
        {
          "command": "imbricate.page.edit",
          "when": "view == imbricate-pages && viewItem == page-item",
          "group": "inline"
        },
        {
          "command": "imbricate.script.create",
          "when": "view == imbricate-scripts && viewItem == script-origin-item",
          "group": "inline"
        },
        {
          "command": "imbricate.script.edit",
          "when": "view == imbricate-scripts && viewItem == script-item",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "package": "vsce package --no-yarn",
    "publish": "vsce publish --no-yarn"
  },
  "dependencies": {
    "@imbricate/core": "^1.10.0",
    "@imbricate/local-fundamental": "^1.2.0",
    "@imbricate/origin-file-system": "^1.8.0",
    "@sudoo/io": "^1.8.0"
  },
  "devDependencies": {
    "@types/mocha": "^10",
    "@types/node": "20",
    "@types/vscode": "^1",
    "@typescript-eslint/eslint-plugin": "^7",
    "@typescript-eslint/parser": "^7",
    "@vscode/test-cli": "^0",
    "@vscode/test-electron": "^2",
    "esbuild": "^0.20.2",
    "eslint": "^8",
    "typescript": "^5"
  }
}